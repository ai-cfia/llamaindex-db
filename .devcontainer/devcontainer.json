{
  "name": "Python 3 with Jupyter",
  "image": "mcr.microsoft.com/devcontainers/python:3.12",
  "postCreateCommand": "pip3 install --user -r requirements.txt",
  "customizations": {
    "vscode": {
      "extensions": [
        "charliermarsh.ruff",
        "GitHub.vscode-pull-request-github",
        "ms-python.python",
        "ms-python.black-formatter",
        "ms-toolsai.jupyter"
      ]
    }
  }
}
